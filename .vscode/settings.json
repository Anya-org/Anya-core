{
    "cSpell.words": [
        "Anya",
        "bip341",
        "bitcoincore",
        "bitcoinjs",
        "huggingface",
        "onnx",
        "ONNX",
        "PKCS",
        "psbt",
        "RUSTSEC",
        "safetensors",
        "Safetensors",
        "schnorr",
        "secp256k1",
        "taproot",
        "utxo"
    ],
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.checkOnSave.extraArgs": [
        "--all-targets",
        "--all-features"
    ],
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "markdownlint.config": {
        "MD013": false,
        "MD033": false,
        "MD040": true,
        "MD047": true
    },
    "chat.mcp.autostart": "onlyNew"
}
